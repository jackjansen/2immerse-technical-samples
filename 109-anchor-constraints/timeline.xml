<tl:document
  xmlns:tl="http://jackjansen.nl/timelines"
  xmlns:tim="http://jackjansen.nl/2immerse"
  xmlns:tic="http://jackjansen.nl/2immerse/component"
  xmlns:tlcheck="http://jackjansen.nl/timelines/check"
  title="Test a single MP4 video">

  <tl:par>

    <tl:seq>
        <tl:par tl:end="master">
            <tl:sleep tl:dur="40" tl:prio="high"/>
            <tl:ref tim:class="layoutTest" tim:dmappcid="TL-01"/>
            <tl:ref tim:class="layoutTest" tim:dmappcid="TR-01"/>
            <tl:ref tim:class="layoutTest" tim:dmappcid="BL-01"/>
            <tl:ref tim:class="layoutTest" tim:dmappcid="BR-01"/>
            <tl:seq tl:prio="high">
                <tl:par tl:end="master">
                  <tl:sleep tl:dur="20" tl:prio="high"/>
                </tl:par>
                <tl:par tl:end="master">
                    <tl:sleep tl:dur="20" tl:prio="high"/>
                    <tl:ref tim:class="layoutTest" tim:dmappcid="L-03"/>
                    <tl:ref tim:class="layoutTest" tim:dmappcid="R-03"/>
                    <tl:ref tim:class="layoutTest" tim:dmappcid="T-03"/>
                    <tl:ref tim:class="layoutTest" tim:dmappcid="B-03"/>
                    <tl:ref tim:class="layoutTest" tim:dmappcid="NA-01"/>
                    <tl:ref tim:class="layoutTest" tim:dmappcid="NA-02"/>
                </tl:par>


            </tl:seq>
        </tl:par>

        <!-- Bug workaround -->
        <tl:sleep tl:dur="1"/>
    </tl:seq>

  </tl:par>

</tl:document>
