<tl:document xmlns:tl="http://jackjansen.nl/timelines" xmlns:tim="http://jackjansen.nl/2immerse" xmlns:tic="http://jackjansen.nl/2immerse/component" xmlns:tlcheck="http://jackjansen.nl/timelines/check" title="Match Clock">
  <tl:par>
    <tl:sleep tl:dur="1000"/>
    <tl:ref tim:class="PrimeDMAppComponent"
            tim:url="https://origin.platform.2immerse.eu/deployment/edge/dmapp-components/prime/prime.html"
            tic:scene="CH_Clock2.pbx"
            tic:action="show"
            tic:timeText="45:00"
            tic:hTeam="TOT"
            tic:aTeam="WBA"
            tic:score="0-0"
            xml:id="clock"
            title="clock"/>
    <tl:seq>
        <tl:sleep tl:dur="20"/>
        <tim:update tim:target="clock" tic:score="1-0"/>
        <tl:sleep tl:dur="10"/>
        <tim:update tim:target="clock" tic:score="2-0"/>
        <tl:sleep tl:dur="10"/>
        <tim:update tim:target="clock" tic:timeText="90:00"/>
        <tl:sleep tl:dur="10"/>
        <tim:update tim:target="clock" tic:timeText="90:00"/>
        <tl:sleep tl:dur="10"/>
        <tim:update tim:target="clock" tic:hTeam="WBA"/>
        <tim:update tim:target="clock" tic:aTeam="TOT"/>
    </tl:seq>
  </tl:par>
</tl:document>
