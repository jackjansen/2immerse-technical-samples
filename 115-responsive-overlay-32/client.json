{
	"description": "Technical Sample 115: Responsive Component Overlay 32\" TV",
	"mode": "tv",
	"serviceUrlPreset": "aws_test",
	"controllerOptions": {
		"deviceIdPrefix": "tv",
		"deviceIdNamespace": "ts-tv",
		"defaultLogLevel": "trace",
		"networkLogLevel": "trace",
		"longFormConsoleLogging": true,
		"showUserErrorMessageUI": true,
		"displayWidth": 1920,
		"displayHeight": 1080,
		"displayResolution": 68.8,
		"concurrentVideo": 4
	},
	"appendHtmlUrl": "appended.html",
	"regions": [
		{
			"id": "stage",
			"elementSelector": "#region-stage",
			"resizable": true,
			"width": "100%",
			"height": "100%"
		},
		{
			"id": "overlay",
			"elementSelector": "#region-overlay",
			"resizable": true,
			"width": "80%",
			"height": "80%"
		}
	],
	"serviceInput": {
		"layout": "layout.json",
		"timeline": "timeline.xml"
	},
	"debugOptions": {
		"debugComponent": true,
		"devLogging": true,
		"failurePlaceholders": true
	},
	"variations": [
		{
			"name": "edge",
			"description": "Use edge services (instead of test)",
			"type": "optional",
			"content": {
				"serviceUrlPreset": "aws_edge"
			}
		},
		{
			"name": "notv",
			"description": "Disable TV emulation (run standalone)",
			"type": "optional",
			"content": {
				"mode": "standalone",
				"controllerOptions": {
					"deviceIdPrefix": "standalone",
					"deviceIdNamespace": "ts-standalone"
				}
			}
		},
		{
			"name": "nodebug",
			"description": "Disable debug",
			"type": "optional",
			"content": {
				"debugOptions": {
					"debugComponent": false,
					"devLogging": false,
					"failurePlaceholders": false
				}
			}
		}
	]
}
